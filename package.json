{
  "name": "starter-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "format": "biome format --write",
    "format:check": "biome format",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "check": "biome check",
    "check:fix": "biome check --write",
    "oxlint": "oxlint src --react-plugin --jsx-a11y-plugin",
    "oxlint:fix": "oxlint src --react-plugin --jsx-a11y-plugin --fix",
    "oxlint:imports": "oxlint src --import-plugin --tsconfig tsconfig.json",
    "oxlint:type-aware": "oxlint src --react-plugin --jsx-a11y-plugin --type-aware",
    "deadcode": "knip --config knip.json --tsConfig tsconfig.json",
    "deadcode:strict": "knip --strict --config knip.json --tsConfig tsconfig.json",
    "exports:unused": "knip --exports --config knip.json --tsConfig tsconfig.json",
    "deps:unused": "depcheck --json",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ci": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "test:people": "vitest src/features/people --run",
    "fix:all": "biome check --write && oxlint src --react-plugin --jsx-a11y-plugin --fix",
    "precommit:fast": "pnpm run fix:all && pnpm run typecheck",
    "precommit:strict": "pnpm run fix:all && pnpm run typecheck && pnpm run deadcode && pnpm run test:people",
    "audit:deps": "depcheck --json || true",
    "verify": "pnpm run format:check && pnpm run lint && pnpm run typecheck && pnpm run oxlint",
    "ci": "pnpm run check && pnpm run typecheck && pnpm run oxlint && pnpm run deadcode:strict && pnpm run test:ci"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query-devtools": "^5.89.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^24.5.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "depcheck": "^1.4.7",
    "jsdom": "^26.1.0",
    "knip": "^5.63.1",
    "msw": "^2.11.2",
    "oxlint": "^1.15.0",
    "oxlint-tsgolint": "^0.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "typescript": "~5.8.3",
    "vite": "^7.1.5",
    "vitest": "^3.2.4",
    "zod-validation-error": "^4.0.1"
  },
  "dependencies": {
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-query": "^5.89.0",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "match-sorter": "^8.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.8"
  }
}