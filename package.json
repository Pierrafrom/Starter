{
  "name": "starter-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "format": "biome format --write",
    "format:check": "biome format",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "check": "biome check",
    "check:fix": "biome check --write",
    "deadcode": "knip --config knip.json --tsConfig tsconfig.json",
    "deadcode:strict": "knip --strict --config knip.json --tsConfig tsconfig.json",
    "exports:unused": "knip --exports --config knip.json --tsConfig tsconfig.json",
    "deps:unused": "depcheck --json",
    "oxlint": "oxlint . --react-plugin --jsx-a11y-plugin",
    "oxlint:fix": "oxlint . --react-plugin --jsx-a11y-plugin --fix",
    "oxlint:imports": "oxlint . --import-plugin --tsconfig tsconfig.json",
    "oxlint:type-aware": "oxlint . --react-plugin --jsx-a11y-plugin --type-aware",
    "test": "vitest",
  "test:ci": "vitest --run",
  "test:watch": "vitest --watch",
  "test:coverage": "vitest --run --coverage",
  "test:people": "vitest src/features/people --run",
  "prepare:table": "echo 'install @tanstack/react-table and match-sorter if missing'",
    "ci": "pnpm run check && pnpm run typecheck && pnpm run oxlint && pnpm run deadcode:strict && pnpm run test:ci"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@hookform/resolvers": "^5.2.1",
    "@tanstack/react-query-devtools": "^5.87.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/strictest": "^2.0.5",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "depcheck": "^1.4.7",
    "jsdom": "^26.1.0",
    "knip": "^5.63.1",
    "msw": "^2.11.2",
    "oxlint": "^1.14.0",
    "oxlint-tsgolint": "^0.2.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "zod-validation-error": "^4.0.1"
  },
  "dependencies": {
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-table": "^8.21.3",
    "match-sorter": "^8.1.0",
    "@tanstack/react-query": "^5.87.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^4.1.5"
  }
}